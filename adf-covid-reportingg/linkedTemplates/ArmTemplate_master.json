{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-covid-reportingg"
		},
		"lksAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lksAzureBlobStorage'"
		},
		"lksAzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lksAzureDataLakeStorage'"
		},
		"lksAzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlcovidreortingg.dfs.core.windows.net/"
		},
		"lksHttpServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"tr_ingent_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_relativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingent_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_sinkFilename": {
			"type": "string",
			"defaultValue": "hospital_admission_json.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-covid-reportingg_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lksAzureBlobStorage_connectionString": {
						"value": "[parameters('lksAzureBlobStorage_connectionString')]"
					},
					"lksAzureDataLakeStorage_accountKey": {
						"value": "[parameters('lksAzureDataLakeStorage_accountKey')]"
					},
					"lksAzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('lksAzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"lksHttpServer_properties_typeProperties_url": {
						"value": "[parameters('lksHttpServer_properties_typeProperties_url')]"
					},
					"tr_ingent_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_relativeURL": {
						"value": "[parameters('tr_ingent_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_relativeURL')]"
					},
					"tr_ingent_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_sinkFilename": {
						"value": "[parameters('tr_ingent_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_sinkFilename')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}